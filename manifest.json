{
  "manifest_version": 2,
  "name": "Bonobo Substitution",
  "description": "Replaces every Boniro with another nickname",
  "homepage_url": "https://github.com/tiagotriques/boniro",
  "version": "1.0",
  "icons": {
    "48": "icons/icon.png",
    "96": "icons/icon@2x.png"
  },

  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["./substitute.js"]
    }
  ]
}
